<script>

    async function handleFileUpload(formDataList, finalFormData) {
        const chunkSize = 5;  // Process in chunks to avoid blocking
        for (let i = 0; i < formDataList.length; i += chunkSize) {
            const chunk = formDataList.slice(i, i + chunkSize);

            // Process each chunk asynchronously
            await appendFormDataAsync(chunk, finalFormData);
            await new Promise(resolve => setTimeout(resolve, 50)); // Yield execution to avoid freezing UI
        }
    }

    async function appendFormDataAsync(formDataList, finalFormData) {
        for (const formData of formDataList) {
            for (const pair of formData.entries()) {
                finalFormData.append(pair[0], pair[1]);
            }
            await new Promise(resolve => setTimeout(resolve, 0));  // Allow other events to execute
        }
    }

</script>



<script>
    function sleepNew(milliseconds) {
        return new Promise(resolve => setTimeout(resolve, milliseconds));
    }
</script>
<script>


    $('#btnSubmitOnline').click(async function () {
        showSpinner();

        try {
            await SaveData(); // Await the completion of the async function
        } catch  {
            console.error("An error occurred while saving data online:", error);
        } finally {
            hideSpinner(); // Hide spinner after the operation completes

            if (Alert_Message == 0) {

                alert("Data Upload Successfully  !!");

                const dbName = "DraftRopeTailDB";
                indexedDB.deleteDatabase(dbName);
                goBack();

            }
        }


    });
</script>


<script>


    function calculateLoaderTiming(formData) {
        debugger
        const baseTime = 1000;
        const numEntries = Array.from(formData.entries()).length;
        if (numEntries < 5) {
            return Math.min(50, baseTime * numEntries);
        }
        if (numEntries < 10) {
            return Math.min(200, baseTime * numEntries);
        }
        if (numEntries < 20) {
            return Math.min(400, baseTime * numEntries);
        }
        if (numEntries < 30) {
            return Math.min(600, baseTime * numEntries);
        }
        if (numEntries < 40) {
            return Math.min(800, baseTime * numEntries);
        }
        else if (numEntries < 50) {
            return Math.min(1000, baseTime * numEntries);
        }
        else if (numEntries < 80) {
            return Math.min(1500, baseTime * numEntries);
        }
        else if (numEntries < 120) {
            return Math.min(1800, baseTime * numEntries);
        }
        else if (numEntries < 140) {
            return Math.min(2000, baseTime * numEntries);
        }
        else if (numEntries > 140) {
            return Math.min(2200, baseTime * numEntries);
        }
    }



    function showSpinner() {
        document.getElementById('cover-spin').style.display = 'flex';
    }

    function hideSpinner() {
        //  $('#cover-spin').hide();
        document.getElementById('cover-spin').style.display = 'none';
    }

    function showSpinnerwithoutBar() {
        document.getElementById('cover-spin1').style.display = 'flex';
    }

    function hideSpinnerwithoutBar() {
        //  $('#cover-spin').hide();
        document.getElementById('cover-spin1').style.display = 'none';
    }

    var bar = 0;

    function updateProgressBar(percentage) {
        debugger
        document.getElementById('progress-bar').style.width = percentage + '%';
        document.getElementById('progress-text').innerText = percentage + '%';
        bar = percentage;

    }


    async function simulateServerProcessing(loaderTime) {
        return new Promise((resolve) => {
            let progress = bar;
            const interval = setInterval(() => {
                if (progress < 100) {
                    progress += 1;
                    bar = progress;
                    if (progress > 100) progress = 100;
                    updateProgressBar(Math.round(progress));
                } else {
                    clearInterval(interval);
                    resolve();
                }
            }, loaderTime); // Use dynamic loader time here
        });
    }

    function simulateProgress(loaderTime) {
        let simulatedProgress = bar;
        simulatedProgressInterval = setInterval(() => {
            if (simulatedProgress < 99) {
                simulatedProgress += 1;
                bar = simulatedProgress;
                updateProgressBar(simulatedProgress);
            }
        }, loaderTime); // Use dynamic loader time here
    }


</script>


<script>


    function extractBase64Data(dataUrl) {

        return dataUrl.split(',')[1];
    }

    function base64ToBytes(base64) {
        const binaryString = window.atob(base64);
        const len = binaryString.length;
        const bytes = new Uint8Array(len);

        for (let i = 0; i < len; i++) {
            bytes[i] = binaryString.charCodeAt(i);
        }

        return bytes;
    }

    function base64ToBlob(base64, contentType = 'image/png') {

        const bytes = base64ToBytes(base64);
        return new Blob([bytes], { type: contentType });
    }

    function blobToFile(blob, fileName) {

        return new File([blob], fileName, { type: blob.type });
    }


</script>

@*<script>
        async function SaveData() {
            const dbName = "DraftRopeTailDB";
            $('#cover-spin').show();

            // Validate required fields
            const Insby = $("#InspectBy").val();
            const Insdt = $("#InspectDate").val();
            if (!Insdt) {
                $('#messageInsdt').text("Inspected Date can not be null !").css("color", "red");
                $('#cover-spin').hide();
                return;
            }
            if (!Insby) {
                $('#messageInsby').text("Please fill name / rank of the person carrying out inspection").css("color", "red");
                $('#cover-spin').hide();
                return;
            }

            // Process each selected inspection
            const inspections = [];
            const inspectionPromises = [];

            $("#tbl1 input[name=case1]:checked").each(function () {
                const inspectionPromise = processInspection($(this), inspections);
                inspectionPromises.push(inspectionPromise);
            });

            // Wait for all inspections to be processed
            await Promise.all(inspectionPromises);

            // Validate the inspections
            const validationErrors = validateInspections(inspections);
            if (validationErrors.length > 0) {
                $('#cover-spin').hide();
                alert(validationErrors.join("\n"));
                return;
            }

            // Split inspections into chunks of 5
            const chunkSize = 5;
            const inspectionChunks = [];
            for (let i = 0; i < inspections.length; i += chunkSize) {
                inspectionChunks.push(inspections.slice(i, i + chunkSize));
            }

            // Process each chunk
            for (const chunk of inspectionChunks) {
                const inspectionsJson = JSON.stringify(chunk);
                try {
                    const result = await $.ajax({
                        url: '@Url.Action("InsertInspection", "TailInspection")',
                        type: "POST",
                        data: { 'inspections': inspectionsJson },
                        dataType: "json",
                        beforeSend: function (x) {
                            if (x && x.overrideMimeType) {
                                x.overrideMimeType("application/json;charset=UTF-8");
                            }
                        }
                    });
                    // Optionally handle result here if needed
                } catch (error) {
                    $('#cover-spin').hide();
                    console.error("AJAX error: ", error);
                    alert("An error occurred while processing your request. Please try again.");
                    return;
                }
            }

            $('#cover-spin').hide();
            indexedDB.deleteDatabase(dbName);
            window.location.href = window.location.origin + result;
        }

        function processInspection($checkbox, inspections) {
            return new Promise(async (resolve) => {
                const MooringRopeInspection = {};
                const row = $checkbox.closest("tr")[0];
                const cells = $checkbox.closest('tr').children('td');
                const cntSrNo = row.cells[20].innerHTML;

                // Default image variables
                let image1 = "", image2 = "", photo1 = "", photo2 = "";
                const count = (cntSrNo - 1) * 2;
                const count1 = count + 1;
                const mydivid_1 = "Maindiv1_" + cntSrNo.trim();
                const mydivid_2 = "Maindiv2_" + cntSrNo.trim();

                // Process Dropzone files
                if (document.getElementById(mydivid_1)?.children.length > 0) {
                    await processFiles($('.dropzone')[count].dropzone, image1, photo1);
                } else {
                    $('.dropzone')[count].dropzone.files.forEach(file => {
                        image1 = file.name;
                        photo1 = file.previewElement.querySelector("img").src;
                    });
                }

                if (document.getElementById(mydivid_2)?.children.length > 0) {
                    await processFiles($('.dropzone')[count1].dropzone, image2, photo2);
                } else {
                    $('.dropzone')[count1].dropzone.files.forEach(file => {
                        image2 = file.name;
                        photo2 = file.previewElement.querySelector("img").src;
                    });
                }

                // Fill in inspection details
                MooringRopeInspection.ExternalRating_A = cells.eq(9).find('select').val();
                MooringRopeInspection.InternalRating_A = cells.eq(10).find('select').val();
                MooringRopeInspection.AverageRating_A = cells.eq(11).find('input').val().trim();
                MooringRopeInspection.RopeId = row.cells[2].innerHTML;
                MooringRopeInspection.WinchId = row.cells[3].innerHTML;
                MooringRopeInspection.LengthOFAbrasion_A = cells.eq(12).find('input').val().trim();
                MooringRopeInspection.DistanceOutboard_A = cells.eq(13).find('input').val().trim();
                MooringRopeInspection.CutYarnCount_A = cells.eq(14).find('input').val().trim();
                MooringRopeInspection.LengthOFGlazing_A = cells.eq(15).find('input').val().trim();
                MooringRopeInspection.Chafe_guard_condition = cells.eq(16).find('select').val();
                MooringRopeInspection.Twist = cells.eq(17).find('input').val();
                MooringRopeInspection.Image1 = image1;
                MooringRopeInspection.Image2 = image2;
                MooringRopeInspection.Photo1 = photo1;
                MooringRopeInspection.Photo2 = photo2;
                MooringRopeInspection.InspectBy = $("#InspectBy").val();
                MooringRopeInspection.InspectDate = $("#InspectDate").val();

                inspections.push(MooringRopeInspection);
                resolve();
            });
        }

        function processFiles(dropzone, image, photo) {
            return new Promise((resolve) => {
                dropzone.files.forEach(file => {
                    image = file.name;
                    photo = file.previewElement.querySelector("img").src;
                });
                resolve();
            });
        }

        function validateInspections(inspections) {
            const errors = [];
            inspections.forEach(inspection => {
                if (!inspection.LengthOFAbrasion_A) {
                    errors.push("Length OF Abrasion(Rating) is empty in Zone A, Please Enter the value");
                }
                if (!inspection.DistanceOutboard_A) {
                    errors.push("Distance from Outboard eye is empty in Zone A, Please Enter the value");
                }
                if (!inspection.CutYarnCount_A) {
                    errors.push("Cut Yarn Count is empty in Zone A, Please Enter the value (meter)");
                }
                if (!inspection.LengthOFGlazing_A) {
                    errors.push("Length OF Glazing is empty in Zone A, Please Enter the value");
                }
                if (!inspection.Chafe_guard_condition) {
                    errors.push("Please select an option from Chafe Guard !");
                }
                if (inspection.AverageRating_A >= 5 && (!inspection.Image1 || !inspection.Image2)) {
                    errors.push(`Due to the reported condition of Line ID ${inspection.RopeId}, it is compulsory to attach at least 1 photograph when rating equal or greater than 5.`);
                }
            });
            return errors;
        }
    </script>*@


