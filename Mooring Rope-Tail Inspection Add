            //}
            sleep(1000);

            $.ajax({
                url: '@Url.Action("InsertInspection", "TailInspection")',
                type: "POST",
                data: { 'inspections': inspections },
                dataType: "json",
                beforeSend: function (x) {
                    if (x && x.overrideMimeType) {
                        x.overrideMimeType("application/j-son;charset=UTF-8");
                    }
                },
