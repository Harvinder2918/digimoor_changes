                $('#cover-spin').hide();
                if (checkValidation == 0) {

                    alert("Please Choose atleast 1 line to inspect by selecting checkbox in the first column !");
                }
                return;
            }

            if (checkValidation == 1) {
                $('#cover-spin').hide();
                return;
            }


