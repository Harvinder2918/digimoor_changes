                    $('#cover-spin').hide();
                    //localData.removeItem("addLineInspection");
                    //alert(result);
                    //sleep(1000);
                    //alert('Data has been Submitted.');
                    indexedDB.deleteDatabase(dbName);
                    window.location.href = window.location.origin + result;

