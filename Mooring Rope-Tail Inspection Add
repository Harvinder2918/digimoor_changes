                        $('.dropzone')[count].dropzone.files.forEach(function (file) {
                            if (photo1 == "" && image1 == "" && file.name == "") {
                                image1 = "";
                                photo1 = "";

                            } else {

                                image1 = file.name;
                                photo1 = file.previewElement.querySelector("img").src;

                            }

                        });
                    } else {
                        photo1 = img1_Element.src;
                        image1 = img1_Element.title;
                    }
                }
                else {
                    $('.dropzone')[count].dropzone.files.forEach(function (file) {

                        image1 = file.name;
                        photo1 = file.previewElement.querySelector("img").src;


                    });
                }

                var mydivid_2 = "Maindiv2_" + cntSrNo.trim();
                var parentElement_2 = document.getElementById(mydivid_2);
                let hasChildNodes_2 = parentElement_2.children.length;

                if (hasChildNodes_2 > 0) {
                    debugger;
                    var img2 = "img2_" + cntSrNo.trim();
                    var img2_Element = document.getElementById(img2);


